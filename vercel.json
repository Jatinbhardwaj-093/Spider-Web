{
  "version": 2,
  "name": "spider-web-rag",
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "Backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/main.py"
    },
    {
      "src": "/health",
      "dest": "Backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/dist/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12"
  },
  "functions": {
    "Backend/main.py": {
      "maxDuration": 60
    }
  }
}
